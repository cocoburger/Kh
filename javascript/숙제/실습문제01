<body>
    <h1> 행 열 갯수 각 각 입력받아 테이블 만들기 </h1>
    

    <button onclick="tableMake();">테이블 생성</button>
    <script>
        tableMake=()=>{
            let count=1;
            let row = window.prompt("행 개수를 입력하세요");
            let col = window.prompt("열 개수를 입력하세요");

            document.write("<table border='1px;'>");
                for(let i=0; i<row; i++){
                    document.write("<tr height='50px'>");
                        for(let j=0; j<col; j++){
                            document.write("<td width='50px'>");
                                document.write(count);
                                count++;
                        }
                }

    }       
    </script>

    <script>
        let test=(()=>{
            //return "aa";
        })();
        function testF(){
            return "aa";
        }
        test=testF();
        console.log(test);
    </script>
